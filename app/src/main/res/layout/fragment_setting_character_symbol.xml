<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:outLine="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.maple_stat.ui.setting.setting_inner_fragment.SettingCharacterSymbolFragment">
    <data>
        <variable
            name="fragment"
            type="com.example.maple_stat.ui.setting.setting_inner_fragment.SettingCharacterSymbolFragment"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_margin="10dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="3dp">

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@drawable/stat_row_textview_left_dark"
                    android:gravity="center"
                    android:text="여로"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="1.0"
                    outLine:textStrokeColor="#000000" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:layout_marginRight="2dp"
                    android:background="@drawable/stat_row_textview_right"
                    android:gravity="center|right"
                    android:text="0"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="0.5"
                    outLine:textStrokeColor="#000000" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:layout_marginLeft="2dp"
                    android:background="@drawable/stat_row_textview_left_dark"
                    android:gravity="center"
                    android:text="츄츄"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="1.0"
                    outLine:textStrokeColor="#000000" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@drawable/stat_row_textview_right"
                    android:gravity="center|right"
                    android:text="0"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="0.5"
                    outLine:textStrokeColor="#000000" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="3dp">

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@drawable/stat_row_textview_left_dark"
                    android:gravity="center"
                    android:text="레헬른"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="1.0"
                    outLine:textStrokeColor="#000000" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:layout_marginRight="2dp"
                    android:background="@drawable/stat_row_textview_right"
                    android:gravity="center|right"
                    android:text="0"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="0.5"
                    outLine:textStrokeColor="#000000" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:layout_marginLeft="2dp"
                    android:background="@drawable/stat_row_textview_left_dark"
                    android:gravity="center"
                    android:text="아르카나"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="1.0"
                    outLine:textStrokeColor="#000000" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@drawable/stat_row_textview_right"
                    android:gravity="center|right"
                    android:text="0"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="0.5"
                    outLine:textStrokeColor="#000000" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="3dp"
                android:paddingBottom="10dp">

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@drawable/stat_row_textview_left_dark"
                    android:gravity="center"
                    android:text="모라스"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="1.0"
                    outLine:textStrokeColor="#000000" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:layout_marginRight="2dp"
                    android:background="@drawable/stat_row_textview_right"
                    android:gravity="center|right"
                    android:text="0"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="0.5"
                    outLine:textStrokeColor="#000000" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:layout_marginLeft="2dp"
                    android:background="@drawable/stat_row_textview_left_dark"
                    android:gravity="center"
                    android:text="에스페라"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="1.0"
                    outLine:textStrokeColor="#000000" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@drawable/stat_row_textview_right"
                    android:gravity="center|right"
                    android:text="0"
                    android:textSize="20dp"
                    android:singleLine="true"
                    outLine:textStroke="true"
                    outLine:textStrokeWidth="0.5"
                    outLine:textStrokeColor="#000000" />

            </TableRow>




        </TableLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>