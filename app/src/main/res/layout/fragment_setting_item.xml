                    android:layout_height="wrap_content"
                    android:layout_columnWeight="1"
                    android:background="@color/colorGray"
                    android:layout_margin="4dp"
                    android:padding="2dp"
                    android:gravity="center">

                    <com.example.maple_stat.custom.CustomTextOutLine
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textSize="15dp"/>

                </com.example.maple_stat.custom.SquareLayout>

                <com.example.maple_stat.custom.SquareLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_columnWeight="1"
                    android:background="@drawable/setting_button_white"
                    android:layout_margin="4dp"
                    android:padding="2dp"
                    android:gravity="center">

                    <com.example.maple_stat.custom.CustomTextOutLine
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ripple_effect_textview"
                        android:gravity="center"
                        android:text="하트"
                        android:textColor="@color/colorPrimaryDark"
                        android:textSize="15dp"
                        app:textStroke="true"
                        app:textStrokeWidth="1.0"
                        app:textStrokeColor="#000000"
                        android:onClick="@{(view) -> fragment.pressItemRing4Button(view)}"
                        android:clickable="true"/>

                </com.example.maple_stat.custom.SquareLayout>

            </GridLayout>

        </LinearLayout>


        <TableLayout
            android:id="@+id/tableLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="3dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@color/colorWhite"
                    android:gravity="center"
                    android:textColor="@color/colorPrimaryDark"
                    android:textStyle="bold"
                    android:text="="
                    android:textSize="20sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="3dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@color/colorWhite"
                    android:textSize="20sp" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@drawable/stat_row_textview_left_green"
                    android:gravity="center"
                    android:text="주스텟"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:singleLine="true"
                    app:textStroke="true"
                    app:textStrokeWidth="1.0"
                    app:textStrokeColor="@color/colorPrimaryDark" />

                <com.example.maple_stat.custom.CustomTextOutLine
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:background="@drawable/stat_row_textview_right"
                    android:gravity="center|right"
                    android:text="0"
                    android:textSize="20dp"
                    android:singleLine="true"
                    app:textStroke="true"
                    app:textStrokeWidth="0.5"
                    app:textStrokeColor="@color/colorPrimaryDark" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@color/colorWhite"
                    android:textSize="20sp" />

            </TableRow>

        </TableLayout>

    </LinearLayout>


</layout>
